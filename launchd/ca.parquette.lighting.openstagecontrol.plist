<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>ca.parquette.lighting.openstagecontrol</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Applications/open-stage-control.app/Contents/MacOS/open-stage-control</string>
		<string>-n</string>
		<string>--config-file</string>
		<string>/ProjectPath/open-stage-control/server.config</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/Users/user/Library/Logs/ca.parquette.lighting.openstagecontrol.out.log</string>
	<key>StandardErrorPath</key>
	<string>/Users/user/Library/Logs/ca.parquette.lighting.openstagecontrol.err.log</string>
</dict>
</plist>
